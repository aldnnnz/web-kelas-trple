<table id="example" class="table table-hover" style="width:100%">
    <thead class="bg-light">
        <tr>
            <th>id</th>
            <th>nama</th>
            <th>time</th>
        </tr>
    </thead>
    <tbody>
        <% users.forEach(user => { %>
        <tr>
            <td><%= user.id %></td>
            <td><%= user.nama %></td>
            <td><%= user.created_at%></td>
        </tr>
        <% }); %>
    </tbody>
</table>
